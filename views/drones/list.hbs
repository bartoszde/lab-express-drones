<h2>Available drones</h2>
<div class="drone-list">
{{#each droneArr}}
  <section class="drone-summary">
    <p>
      Name:
      {{this.name}}
      <br />
      Propellers:
      {{this.propellers}}
      <br />
      Rating:
      {{this.maxSpeed}}
      <br />
      <a href="/drones/{{_id}}/edit">Edit</a>
          <form action="/drones/{{_id}}/delete" method="POST">
      <button>Delete</button>
    </form>
    </p>
    

  </section>
{{/each}}

</div>